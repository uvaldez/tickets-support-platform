{
  "name": "temporal-schedules",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "lint": "eslint .",
    "start": "ts-node src/worker.ts",
    "start.watch": "nodemon src/worker.ts",
    "schedule.start": "ts-node src/start-schedule.ts",
    "schedule.go-faster": "ts-node src/go-faster.ts",
    "schedule.pause": "ts-node src/pause-schedule.ts",
    "schedule.unpause": "ts-node src/unpause-schedule.ts",
    "schedule.delete": "ts-node src/delete-schedule.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@temporalio/activity": "^1.11.1",
    "@temporalio/client": "^1.11.1",
    "@temporalio/worker": "^1.11.1",
    "@temporalio/workflow": "^1.11.1",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.0",
    "@types/node": "^16.11.43",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.8.8",
    "ts-node": "^10.8.1",
    "typescript": "^4.4.2"
  }
}
